/**
 * Exclusive-or gate:
 * out = not (a == b)
 */

CHIP Xor {
    IN a, b;
    OUT out;

    PARTS:
    Not(in=a, out=na);
    Not(in=b, out=nb);
    And(a=a, b=nb, out=a_and_nb);
    And(a=na, b=b, out=na_and_b);
    Or(a=a_and_nb, b=na_and_b, out=out);
}