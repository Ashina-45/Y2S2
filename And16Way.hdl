// File name: And16Way.hdl

/**
 * 16-way And: 
 * out = (in[0] and in[1] and ... and in[15])
 */

CHIP And16Way {
    IN in[16];
    OUT out;

    PARTS:
    Not16(in=in, out=ni);
    Or8Way(in=ni[0..7],  out=z0);
    Or8Way(in=ni[8..15], out=z1);
    Or(a=z0, b=z1, out=any);
    Not(in=any, out=out);
}


